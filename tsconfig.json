{
  "compilerOptions": {
    "target": "ES5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "out/",
    "rootDir": "src/",
    "resolveJsonModule": true
  },
  "include": [
    "src/**/*"
  ]
}